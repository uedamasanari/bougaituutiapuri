      function movefunk(time){
        Cslime[0]=slime[0];
        Cslime[1]=slime[1];
        Cslime[2]=slime[2];
        Cdemon[0]=demon[0];
        Cdemon[1]=demon[1];
        Cdemon[2]=demon[2];
        Cdragon=dragon;
        if(KAI!=1){
        if(slime[0]==0&&slime[1]==0&&slime[2]==0&&demon[0]==0&&demon[1]==0&&demon[2]==0&&dragon==0){
          hyoujifunk();
        }else{
          if(slime[0]!=0){
          count=0;
          atackfunk(1,0,0);
          if(tarn%3==0){
            time=500;
          }
          if(slime[0]!=0){
          if(slime[0]!=now){
          }
          do{
          do{
          do{
          do{
          do{
          do{
          do{
          do{
          do{
          do{
            if(count>=1){
              if(move==1){
              slime[0]=slime[0]-1;
              }else if(move==2){
                slime[0]=slime[0]+1;
              }else if(move==3){
                slime[0]=slime[0]-10;
              }else if(move==4){
                slime[0]=slime[0]+10;
              }
            }
            move = Math.floor( Math.random() * 4 ) + 1;
            if(move==1){
              slime[0]=slime[0]+1;
