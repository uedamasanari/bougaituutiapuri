                Start = document.getElementById(clone);
                Start.src="PNG/Draatack.png";
                }
                clone=clone-9;
              }
              clone=dragon+9;
              while(clone>=11 && clone<=99 && clone%10!=0){
                if(syoutotufunk(clone,3,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Draatack.png";
                }
                clone=clone+9;
              }
            }
            if(Mwaza==2){
            }
            if(Mwaza==3){
              
            }
            if(Mwaza==4){
              
            }
            }
            }
          }
        }
        window.setTimeout(function(){
        for(i=11;i<=99;i++){
          if(i%10!=0){
            Start = document.getElementById(i);
            Start.src="PNG/1.png";
          }
        }
        Start = document.getElementById(now);
        Start.src=nowpng;
        if(next!=0){
        Start = document.getElementById(next);
        Start.src="PNG/6.png";
        }
        if(takara!=0){
          Start = document.getElementById(takara);
          Start.src="PNG/2.png";
        }
        if(slime[0]!=0){
        Start = document.getElementById(slime[0]);
        Start.src="PNG/3.png";
        }
        if(slime[1]!=0){
        Start = document.getElementById(slime[1]);
        Start.src="PNG/3.png";
        }
        if(slime[2]!=0){
        Start = document.getElementById(slime[2]);
        Start.src="PNG/3.png";
        }
        if(demon[0]!=0){
          Start = document.getElementById(demon[0]);
          Start.src="PNG/4.png";
        }
        if(demon[1]!=0){
          Start = document.getElementById(demon[1]);
          Start.src="PNG/4.png";
        }
        if(demon[2]!=0){
          Start = document.getElementById(demon[2]);
          Start.src="PNG/4.png";
        }
        if(dragon!=0){
          Start = document.getElementById(dragon);
          Start.src="PNG/5.png";
        }
        },500);
        }
        if(hantei>=100){
        if(hantei==100){
            //1が縦2が横3が左斜め4右斜め
            if(skill==1){
              MP=MP-10;
              clone=now - 10;
              while(clone>=11){
                if(syoutotufunk(clone,100,1)!=true &&clone>=11){
