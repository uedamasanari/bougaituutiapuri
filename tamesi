      }
      function takarafunk(){
        takara=0;
        Rtakara=Math.floor( Math.random() * 99 ) + 1;
        if(Rtakara>=1&&Rtakara<=29){
          HP=HP+(Math.floor(KAI/10)+1)*10;
          if(MAXHP<HP){
            HP=MAXHP;
          }
          Thyouji=(Math.floor(KAI/10)+1)*10;
          Kai = document.getElementById("log");
          Kai.innerHTML="HPが"+Thyouji+"回復しました！";
          Kai = document.getElementById("hp");
          Kai.innerHTML="　HP"+HP;
        }
        if(Rtakara>=30&&Rtakara<=49){
          HP=HP+(KAI-1);
          MAXHP=MAXHP+(KAI-1);
          Thyouji=(KAI-1);
          Kai = document.getElementById("log");
          Kai.innerHTML="HPが"+Thyouji+"上がりました！";
          Kai = document.getElementById("hp");
            Kai.innerHTML="　HP"+HP;
            Kai = document.getElementById("maxhp");
            Kai.innerHTML="/"+MAXHP;
        }else if(Rtakara>=50&&Rtakara<=69){
          ATK=ATK+Math.floor(KAI/2);
          Thyouji=Math.floor(KAI/2);
          Kai = document.getElementById("log");
          Kai.innerHTML="ATKが"+Thyouji+"上がりました！";
          Kai = document.getElementById("atk");
          Kai.innerHTML="　ATK"+ATK;
        }else if(Rtakara>=70&&Rtakara<=89){
          MP=MP+2;
          MAXMP=MAXMP+2;
          Thyouji=2;
          Kai = document.getElementById("log");
          Kai.innerHTML="MPが"+Thyouji+"上がりました！";
          Kai = document.getElementById("mp");
          Kai.innerHTML="　MP"+MP;
          Kai = document.getElementById("maxmp");
          Kai.innerHTML="/"+MAXMP;
        }else if(Rtakara==98){
          HP=HP+3*(KAI-1);
          MAXHP=MAXHP+3*(KAI-1);
          Thyouji=3*(KAI-1);
          Kai = document.getElementById("log");
          Kai.innerHTML="【激レア】HPが"+Thyouji+"上がりました！";
          Kai = document.getElementById("hp");
          Kai.innerHTML="　HP"+HP;
          Kai = document.getElementById("maxhp");
          Kai.innerHTML="/"+MAXHP;
        }else if(Rtakara==99){
          ATK=ATK+(KAI-1);
          Thyouji=(KAI-1);
          Kai = document.getElementById("log");
          Kai.innerHTML="【激レア】ATKが"+Thyouji+"上がりました！";
          Kai = document.getElementById("atk");
          Kai.innerHTML="　ATK"+ATK;
        }else if(Rtakara==100){
          MP=MP+10;
          MAXMP=MAXMP+10;
          Thyouji=10;
          Kai = document.getElementById("log");
          Kai.innerHTML="【激レア】MPが"+Thyouji+"上がりました！";
          Kai = document.getElementById("mp");
          Kai.innerHTML="　MP"+MP;
          Kai = document.getElementById("maxmp");
          Kai.innerHTML="/"+MAXMP;
        }else if(Rtakara==97){
          Start = document.getElementById(now);
          Start.src="PNG/sword.png";
          nowpng="PNG/sword.png";
          ATK=ATK+2*(KAI/2);
          Bhyouji=2*(KAI/2);
          Bhantei=1;
          Kai = document.getElementById("log");
          Kai.innerHTML="【激レア】剣を装備しました:攻撃力"+Bhyouji;
          Kai = document.getElementById("syoukyo");
          Kai.src="PNG/syoukyo.png";
        }else if(Rtakara==96){
          Start = document.getElementById(now);
          Start.src="PNG/shield.png";
          nowpng="PNG/shield.png";
          Bhyouji=5*(KAI-1);
          Bhantei=2;
          Kai = document.getElementById("log");
          Kai.innerHTML="【激レア】盾を装備しました:防御力"+Bhyouji;
          Kai = document.getElementById("syoukyo");
          Kai.src="PNG/syoukyo.png";
        }else if(Rtakara>=90&&Rtakara<=95){
          Rtakara=Math.floor( Math.random() * 6 ) + 1;
          if(Rtakara==1){
            window.setTimeout(function(){
            Kai = document.getElementById("getskil");
            Kai.src="skil/tate.png";
            Kai = document.getElementById("no");
            Kai.src="PNG/no.png";
            document.getElementById("down").style.display="none";
            document.getElementById('getskil').style.display = "block";
            document.getElementById('no').style.display = "block";
            GETskil=1;
            GETmode=1;
            Kai = document.getElementById("log");
            Kai.innerHTML="入れ替えたいスキルかごみ箱をタップで選択";
            Kai = document.getElementById("evlog");
            Kai.innerHTML="新スキル【縦】をゲットしました！";
            },510);
