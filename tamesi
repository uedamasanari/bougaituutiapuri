      function syoutotufunk(C,S,H){
        if(S==1){
        if(now==C){
          damagefunk(S);
          if(Bhantei==0){
            Start = document.getElementById(C);
            Start.src="PNG/JdamageS.png";
            return true;
          }else if(Bhantei==1){
            Start = document.getElementById(C);
            Start.src="PNG/JdamageSS.png";
            return true;
          }else if(Bhantei==2){
            Start = document.getElementById(C);
            Start.src="PNG/JdamageST.png";
            return true;
          }
        }
        if(C==Cslime[0]){
          Start = document.getElementById(Cslime[0]);
          Start.src="PNG/SkaihukuS.png";
          return true;
        }
        if(C==Cslime[1]){
          Start = document.getElementById(Cslime[1]);
          Start.src="PNG/SkaihukuS.png";
          return true;
        }
        if(C==Cslime[2]){
          Start = document.getElementById(Cslime[2]);
          Start.src="PNG/SkaihukuS.png";
          return true;
        }
        if(C==Cdemon[0]){
          Start = document.getElementById(Cdemon[0]);
          Start.src="PNG/DkaihukuS.png";
          return true;
        }
        if(C==Cdemon[1]){
          Start = document.getElementById(Cdemon[1]);
          Start.src="PNG/DkaihukuS.png";
          return true;
        }
        if(C==Cdemon[2]){
          Start = document.getElementById(Cdemon[2]);
          Start.src="PNG/DkaihukuS.png";
          return true;
        }
        if(C==dragon){
          Start = document.getElementById(Cdragon);
          Start.src="PNG/DrakaihukuS.png";
          return true;
        }
        if(C==takara){
          takara=0;
        }
        if(C==next){
          clone=-999;
        }
        }else if(S==2){
          if(now==C){
          damagefunk(S);
          if(Bhantei==0){
            Start = document.getElementById(C);
            Start.src="PNG/JdamageD.png";
            return true;
          }else if(Bhantei==1){
            Start = document.getElementById(C);
            Start.src="PNG/JdamageDS.png";
            return true;
          }else if(Bhantei==2){
            Start = document.getElementById(C);
            Start.src="PNG/JdamageDT.png";
            return true;
          }
        }
        if(C==Cslime[0]){
          Start = document.getElementById(Cslime[0]);
          Start.src="PNG/SkaihukuD.png";
          return true;
        }
        if(C==Cslime[1]){
          Start = document.getElementById(Cslime[1]);
          Start.src="PNG/SkaihukuD.png";
          return true;
        }
        if(C==Cslime[2]){
          Start = document.getElementById(Cslime[2]);
          Start.src="PNG/SkaihukuD.png";
          return true;
        }
        if(C==Cdemon[0]){
          Start = document.getElementById(Cdemon[0]);
          Start.src="PNG/DkaihukuD.png";
          return true;
        }
        if(C==Cdemon[1]){
          Start = document.getElementById(Cdemon[1]);
          Start.src="PNG/DkaihukuD.png";
          return true;
        }
        if(C==Cdemon[2]){
          Start = document.getElementById(Cdemon[2]);
          Start.src="PNG/DkaihukuD.png";
          return true;
        }
        if(C==dragon){
          Start = document.getElementById(Cdragon);
          Start.src="PNG/DrakaihukuD.png";
          return true;
        }
        if(C==takara){
          takara=0;
        }
        if(C==next){
          clone=-999;
        }
        }else if(S==3){
          if(now==C){
          damagefunk(S);
          if(Bhantei==0){
            Start = document.getElementById(C);
            Start.src="PNG/JdamageDra.png";
            return true;
          }else if(Bhantei==1){
            Start = document.getElementById(C);
            Start.src="PNG/JdamageDraS.png";
            return true;
          }else if(Bhantei==2){
            Start = document.getElementById(C);
            Start.src="PNG/JdamageDraT.png";
            return true;
          }
        }
        if(C==Cslime[0]){
          Start = document.getElementById(Cslime[0]);
          Start.src="PNG/SkaihukuDra.png";
          return true;
        }
        if(C==Cslime[1]){
          Start = document.getElementById(Cslime[1]);
          Start.src="PNG/SkaihukuDra.png";
          return true;
        }
        if(C==Cslime[2]){
          Start = document.getElementById(Cslime[2]);
          Start.src="PNG/SkaihukuDra.png";
          return true;
        }
        if(C==Cdemon[0]){
          Start = document.getElementById(Cdemon[0]);
          Start.src="PNG/DkaihukuDra.png";
          return true;
        }
        if(C==Cdemon[1]){
          Start = document.getElementById(Cdemon[1]);
          Start.src="PNG/DkaihukuDra.png";
          return true;
        }
        if(C==Cdemon[2]){
          Start = document.getElementById(Cdemon[2]);
          Start.src="PNG/DkaihukuDra.png";
          return true;
        }
        }else if(S==11){
          if(now==C){
            Start = document.getElementById(C);
            Start.src=nowpng;
          }else if(next==C){
            if(H==1){
              clone=-999;
            }else if(H==2){
              clone2=-999;
            }
          }
        }else if(S==100){
        for(i=0;i<=2;i++){
        if(slime[i]==C){
          damagefunk(101,i);
          Start = document.getElementById(C);
          Start.src="PNG/Sdamage.png";
          return true;
        }
        }
        for(i=0;i<=2;i++){
        if(C==demon[i]){
          damagefunk(102,i);
          Start = document.getElementById(C);
          Start.src="PNG/Ddamage.png";
          return true;
        }
        }
        if(C==dragon){
          damagefunk(103);
          Start = document.getElementById(C);
          Start.src="PNG/Dradamage.png";
          return true;
        }else if(C==takara){
          takara=0;
          return true;
        }else if(C==next){
          return true;
        }else if(C==Dratakara){
          return true;
        }
        }
      }
      function damagefunk(D,I){
        if(D==1){
          if(Bhantei==2){
            if((10+2*(KAI-2))-Bhyouji>=0){
              HP=HP-(10+2*(KAI-2));
              Thyouji=(10+2*(KAI-2))-Bhyouji;
              Kai = document.getElementById("log");
              Kai.innerHTML="スライムは"+Thyouji+"ダメージを与えた！";
            }else{
              Kai = document.getElementById("log");
              Kai.innerHTML="スライムはダメージを与えられなかった";
            }
          }else{
            HP=HP-(10+2*(KAI-2));
            Thyouji=(10+2*(KAI-2));
            Kai = document.getElementById("log");
            Kai.innerHTML="スライムは"+Thyouji+"ダメージを与えた！";
          }
          Kai = document.getElementById("hp");
          Kai.innerHTML="　HP　"+ HP;
        }else if(D==2){
          if(Bhantei==2){
            if((10+3*(KAI-2))-Bhyouji>=0){
              HP=HP-(10+3*(KAI-2));
              Thyouji=(10+3*(KAI-2))-Bhyouji;
              Kai = document.getElementById("log");
              Kai.innerHTML="デーモンは"+Thyouji+"ダメージを与えた！";
            }else{
              Kai = document.getElementById("log");
              Kai.innerHTML="デーモンはダメージを与えられなかった";
            }
          }else{
            HP=HP-(10+3*(KAI-2));
            Thyouji=(10+3*(KAI-2));
            Kai = document.getElementById("log");
            Kai.innerHTML="デーモンは"+Thyouji+"ダメージを与えた！";
          }
          Kai = document.getElementById("hp");
          Kai.innerHTML="　HP　"+ HP;
        }else if(D==3){
          if(Bhantei==2){
            if((10+5*(KAI-2))-Bhyouji>=0){
              HP=HP-(10+5*(KAI-2));
              Thyouji=(10+5*(KAI-2))-Bhyouji;
              Kai = document.getElementById("log");
              Kai.innerHTML="ドラゴンは"+Thyouji+"ダメージを与えた！";
            }else{
              Kai = document.getElementById("log");
              Kai.innerHTML="ドラゴンはダメージを与えられなかった";
            }
          }else{
            HP=HP-(10+5*(KAI-2));
            Thyouji=(10+5*(KAI-2));
            Kai = document.getElementById("log");
            Kai.innerHTML="ドラゴンは"+Thyouji+"ダメージを与えた！";
          }
          Kai = document.getElementById("hp");
          Kai.innerHTML="　HP　"+ HP;
        }else if(D==4){
        }else if(D==101){
          slimeHP[I]=slimeHP[I]-ATK;
          if(slimeHP[I]<=0){
            slime[I]=0;
            winfunk(1);
          }
        }else if(D==102){
          demonHP[I]=demonHP[I]-ATK;
          if(demonHP[I]<=0){
            demon[I]=0;
            winfunk(2);
          }
        }else if(D==103){
          dragonHP=dragonHP-ATK;
          if(dragonHP<=0){
            dragon=0;
            winfunk(3);
          }
        }
      }
      function winfunk(W){
        if(W==1){
          keikenti=keikenti+1*(Math.floor(KAI/10)+1);
          coin+=2;
          Kai = document.getElementById("coin");
          Kai.innerHTML=coin;
          while(keikenti>=nextLV){
            LVUPfunk();
          }
        }else if(W==2){
          keikenti=keikenti+2*(Math.floor(KAI/10)+1);
          coin+=5;
          Kai = document.getElementById("coin");
          Kai.innerHTML=coin;
          while(keikenti>=nextLV){
          LVUPfunk();
          }
        }else if(W==3){
          keikenti=keikenti+10*(Math.floor(KAI/10)+1);
          coin+=50;
          Kai = document.getElementById("coin");
          Kai.innerHTML=coin;
          while(keikenti>=nextLV){
          LVUPfunk();
          }
          if(slime[0]!=0){
          slime[0]=0;
          }
          if(demon[0]!=0){
          demon[0]=0;
          }
          if(now!=55){
            Dratakara=55;
            if(now!=25){
              next=25;
              Kai = document.getElementById(next);
              Kai.src="PNG/6.png";
            }else{
              next=15;
              Kai = document.getElementById(next);
              Kai.src="PNG/6.png";
            }
          }else{
            Dratakara=45;
            next=25;
            Kai = document.getElementById(next);
            Kai.src="PNG/6.png";
          }
        }
        Kai = document.getElementById("nextlv");
        Kai.innerHTML=nextLV-keikenti;
      }
      function LVUPfunk(){
        keikenti=keikenti-nextLV;
            Math.floor(nextLV*1.5);
            LV++;
            nextLV=Math.floor(nextLV*1.5);
            MAXHP=MAXHP+10;
            HP=HP+10;
            ATK=ATK+3;
            MAXMP=MAXMP+10;
            MP=MP+2;
            Kai = document.getElementById("hp");
            Kai.innerHTML="　HP"+HP;
            Kai = document.getElementById("maxhp");
            Kai.innerHTML="/"+MAXHP;
            Kai = document.getElementById("atk");
            Kai.innerHTML="　ATK　"+ATK;
            Kai = document.getElementById("mp");
            Kai.innerHTML=" MP　"+MP;
            Kai = document.getElementById("maxmp");
            Kai.innerHTML="/"+MAXMP;
            Kai = document.getElementById("lv");
            Kai.innerHTML="　LV"+LV;
      }
