        localStorage.removeItem('av');
        localStorage.removeItem('aw');
        localStorage.removeItem('ax');
        localStorage.removeItem('ay');
        localStorage.removeItem('az');
        localStorage.removeItem('aaa');
        localStorage.removeItem('aab');
        localStorage.removeItem('aac');
        localStorage.removeItem('aad');
        localStorage.removeItem('aae');
        localStorage.removeItem('aaf');
        localStorage.removeItem('aag');
        localStorage.removeItem('coin');
        localStorage.setItem('wKAI',KAI);
        localStorage.setItem('whp',MAXHP);
        localStorage.setItem('watk',ATK);
        localStorage.setItem('wmp',MAXMP);
        localStorage.setItem('wlv',LV);
        localStorage.setItem('ww',Bhantei);
        localStorage.setItem('ws1',skil1);
        localStorage.setItem('ws2',skil2);
        localStorage.setItem('wtokusyu',tokusyu);
        hihyoujifunk();
        Kai = document.getElementById("save");
        Kai.src="PNG/end.png";
      }
      function skilfunk(){
        tarn++;
        logfunk();
        if(KAI!=1){
        if(GETmode==0){
        if(MP>=10){
        hihyoujifunk();
        atackfunk(100,skil1);
        Kai = document.getElementById("mp");
        Kai.innerHTML=" MP　"+MP;
        }else{
          Kai = document.getElementById("mp");
          Kai.innerHTML=" MP　"+MP;
          Kai = document.getElementById("log");
          Kai.innerHTML="MPが足りません";
        }
        }else{
          skil1=Rtakara;
          GETmode=0;
          Kai = document.getElementById("log");
          Kai.innerHTML="スキルを入れ替えました";
          document.getElementById('getskil').style.display = "none";
          document.getElementById('no').style.display = "none";
          hyoujifunk();
          if(skil1==1){
            Kai = document.getElementById("s1");
            Kai.src="skil/tate.png";
          }else if(skil1==2){
            Kai = document.getElementById("s1");
            Kai.src="skil/yoko.png";
          }else if(skil1==3){
            Kai = document.getElementById("s1");
            Kai.src="skil/hidarinaname.png";
          }else if(skil1==4){
            Kai = document.getElementById("s1");
            Kai.src="skil/miginaname.png";
          }else if(skil1==5){
            Kai = document.getElementById("s1");
            Kai.src="skil/juuji.png";
          }else if(skil1==6){
            Kai = document.getElementById("s1");
            Kai.src="skil/nanamejuuji.png";
          }else if(skil1==7){
            Kai = document.getElementById("s1");
            Kai.src="skil/mawari.png";
          }else if(skil1==8){
            Kai = document.getElementById("s1");
            Kai.src="skil/zennhoui.png";
          }
        }
        }
      }
