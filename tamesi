      function atackfunk(hantei,skill,AI){
        if(hantei<100){
        if(hantei==1){
          if(tarn%3==0&&tarn!=0){
            //1が縦2が横3が左斜め4右斜め
            Mwaza = Math.floor( Math.random() * 4 ) + 1;
            if(slime[AI]!=0){
            if(Mwaza==1){
              clone=slime[AI] - 10;
              while(clone>=11){
                if(syoutotufunk(clone,1,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Satack.png";
                }
                clone=clone-10;
              }
              clone=slime[AI]+10;
              while(clone>=11 && clone<=99){
                if(syoutotufunk(clone,1,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Satack.png";
                }
                clone=clone+10;
              }
            }
            if(Mwaza==2){
              clone=slime[AI] - 1;
              while(clone%10!=0){
                if(syoutotufunk(clone,1,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Satack.png";
                }
                clone=clone-1;
              }
              clone=slime[AI]+1;
              while(clone%10!=0){
                if(syoutotufunk(clone,1,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Satack.png";
                }
                clone=clone+1;
              }
            }
            if(Mwaza==3){
              clone=slime[AI] - 11;
              while(clone>=11 && clone<=99 && clone%10!=0){
                if(syoutotufunk(clone,1,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Satack.png";
                }
                clone=clone-11;
              }
              clone=slime[AI]+11;
              while(clone>=11 && clone<=99 && clone%10!=0){
                if(syoutotufunk(clone,1,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Satack.png";
                }
                clone=clone+11;
              }
            }
            if(Mwaza==4){
              clone=slime[AI] - 9;
              while(clone>=11 && clone%10!=0 &&clone<=99){
                if(syoutotufunk(clone,1,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Satack.png";
                }
                clone=clone-9;
              }
              clone=slime[AI]+9;
              while(clone>=11 && clone<=99 && clone%10!=0){
                if(syoutotufunk(clone,1,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Satack.png";
                }
                clone=clone+9;
              }
            }
            }
          }
        }
        if(hantei==2&&tarn!=0){
          if(tarn%3==0){
            //1が縦2が横3が左斜め4右斜め
            Mwaza = Math.floor( Math.random() * 4 ) + 1;
            if(demon[AI]!=0){
            if(Mwaza==1){
              clone=demon[AI] - 10;
              while(clone>=11){
                if(syoutotufunk(clone,2,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Datack.png";
                }
                clone=clone-10;
              }
              clone=demon[AI]+10;
              while(clone>=11 && clone<=99){
                if(syoutotufunk(clone,2,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Datack.png";
                }
                clone=clone+10;
              }
            }
            if(Mwaza==2){
              clone=demon[AI] - 1;
              while(clone%10!=0){
                if(syoutotufunk(clone,2,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Datack.png";
                }
                clone=clone-1;
              }
              clone=demon[AI]+1;
              while(clone%10!=0){
                if(syoutotufunk(clone,2,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Datack.png";
                }
                clone=clone+1;
              }
            }
            if(Mwaza==3){
              clone=demon[AI] - 11;
              while(clone>=11 && clone<=99 && clone%10!=0){
                if(syoutotufunk(clone,2,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Datack.png";
                }
                clone=clone-11;
              }
              clone=demon[AI]+11;
              while(clone>=11 && clone<=99 && clone%10!=0){
                if(syoutotufunk(clone,2,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Datack.png";
                }
                clone=clone+11;
              }
            }
            if(Mwaza==4){
              clone=demon[AI] - 9;
              while(clone>=11 && clone%10!=0 &&clone<=99){
                if(syoutotufunk(clone,2,1)!=true &&clone>=11){
                Start = document.getElementById(clone);
                Start.src="PNG/Datack.png";
                }
                clone=clone-9;
              }
