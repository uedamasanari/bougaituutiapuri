      function Ssyoukyofunk(){
        if(Bhantei==1){
          ATK=ATK-Bhyouji;
          Kai = document.getElementById("log");
          Kai.innerHTML="剣を捨てました";
          Kai = document.getElementById("atk");
          Bhantei=0;
          nowpng="PNG/100.png";
        }else if(Bhantei==2){
          Kai = document.getElementById("log");
          Kai.innerHTML="盾を捨てました";
          Bhantei=0;
          nowpng="PNG/100.png";
        }
        Kai = document.getElementById(now);
        Kai.src=nowpng;
        Kai = document.getElementById("syoukyo");
        Kai.src="skil/none.png";
      }
      function upfunk(){
        Kai = document.getElementById("log");
        Kai.innerHTML="上に動きます";
        if(KAI!=1){
        hihyoujifunk();
        }
        if(MAXMP>MP){
          MP+=3;
          if(MAXMP<MP){
            MP=MAXMP;
          }
        }
        clearfunk();
        now = now - 10;
        if(next == now){
          setfunk();
        }else{
        if(now >=10 &&  now != slime[0]&& now != slime[1]&& now != slime[2]&& now != demon[0]&& now != demon[1]&& now != demon[2]&& now != dragon){
          nextfunk();
          if(now==takara){
            takarafunk();
            movefunk(500);
          }else if(now==Dratakara){
            Dratakarafunk();
            movefunk(500);
          }else{
            movefunk(500);
          }
        }else{
          now = now + 10;
          nextfunk();
          movefunk(500);
        }
        }
        logfunk();
        Kai = document.getElementById("mp");
        Kai.innerHTML=" MP　"+MP;
      }
      function downfunk(){
        Kai = document.getElementById("log");
        Kai.innerHTML="下に動きます";
        if(KAI!=1){
        hihyoujifunk();
        }
        if(MAXMP>MP){
          MP+=3;
          if(MAXMP<MP){
            MP=MAXMP;
          }
        }
        clearfunk();
        now = now + 10;
        if(next == now){
          setfunk();
        }else{
          if(now <=100 &&  now != slime[0]&& now != slime[1]&& now != slime[2]&& now != demon[0]&& now != demon[1]&& now != demon[2]&& now != dragon){
            nextfunk();
            if(now==takara){
              takarafunk();
              movefunk(500);
            }else if(now==Dratakara){
              Dratakarafunk();
              movefunk(500);
            }else{
              movefunk(500);
            }
          }else{
             now = now - 10;
             nextfunk();
             movefunk(500);
          }
        }
        logfunk();
        Kai = document.getElementById("mp");
        Kai.innerHTML=" MP　"+MP;
      }
      function leftfunk(){
        Kai = document.getElementById("log");
        Kai.innerHTML="左に動きます";
        if(KAI!=1){
        hihyoujifunk();
        }
        if(MAXMP>MP){
          MP+=3;
          if(MAXMP<MP){
            MP=MAXMP;
          }
        }
        clearfunk();
        now = now - 1;
        if(next == now){
          setfunk();
        }else{
          if(now%10 > 0 &&  now != slime[0]&& now != slime[1]&& now != slime[2]&& now != demon[0]&& now != demon[1]&& now != demon[2]&& now != dragon){
            nextfunk();
            if(now==takara){
              takarafunk();
              movefunk(500);
            }else if(now==Dratakara){
              Dratakarafunk();
              movefunk(500);
            }else{
              movefunk(500);
            }
          }else{
            now = now+1;
            nextfunk();
            movefunk(500);
          }
        }
        logfunk();
        Kai = document.getElementById("mp");
        Kai.innerHTML=" MP　"+MP;
      }
      function rightfunk(){
        Kai = document.getElementById("log");
        Kai.innerHTML="右に動きます";
        if(KAI!=1){
        hihyoujifunk();
        }
        if(MAXMP>MP){
          MP+=3;
          if(MAXMP<MP){
            MP=MAXMP;
          }
        }
        clearfunk();
        now = now + 1;
        if(next == now){
          setfunk();
        }else{
          if(now%10 != 0 &&  now != slime[0]&& now != slime[1]&& now != slime[2]&& now != demon[0]&& now != demon[1]&& now != demon[2]&& now != dragon){
            nextfunk();
            if(now==takara){
              takarafunk();
              movefunk(500);
            }else if(now==Dratakara){
              Dratakarafunk();
              movefunk(500);
            }else{
              movefunk(500);
            }
          }else{
            now = now - 1;
            nextfunk();
            movefunk(500);
          }
        }
        logfunk();
        Kai = document.getElementById("mp");
        Kai.innerHTML=" MP　"+MP;
      }
      function logfunk(){
        if(KAI!=1){
        if(slime[0]!=0||slime[1]!=0||slime[2]!=0){
          if(tarn%3==0){
            Start = document.getElementById("tekilog");
            Start.innerHTML="敵の攻撃まであと2ターンです";
          }else if(tarn%3==1){
            Start = document.getElementById("tekilog");
            Start.innerHTML="敵の攻撃まであと1ターンです";
          }else if(tarn%3==2){
            Start = document.getElementById("tekilog");
            Start.innerHTML="【警告】敵の攻撃が来ます";
          }
        }
        if(KAI>30){
          if(demon[0]!=0||demon[1]!=0||demon[2]!=0){
          if(tarn%2==1){
            Start = document.getElementById("tekilog");
            Start.innerHTML="【警告】敵の攻撃が来ます";
          }else if(tarn%2==0){
            Start = document.getElementById("tekilog");
            Start.innerHTML="敵の攻撃まであと1ターンです";
          }
          if(slime[0]!=0||slime[1]!=0||slime[2]!=0){
          if(tarn%3==2){
            Start = document.getElementById("tekilog");
            Start.innerHTML="【警告】敵の攻撃が来ます";
          }
          }
          }
        }
        if(dragon!=0){
        if(KAI%30==0){
          if(tarn%2==1){
            Start = document.getElementById("tekilog");
            Start.innerHTML="【警告】敵の攻撃が来ます";
          }else if(tarn%2==0){
            Start = document.getElementById("tekilog");
            Start.innerHTML="敵の攻撃まであと1ターンです";
          }
        }
        }
        }
      }
      function hihyoujifunk(){
        document.getElementById('up').style.display= "none";
        document.getElementById('down').style.display = "none";
        document.getElementById('left').style.display = "none";
        document.getElementById('right').style.display = "none";
        document.getElementById('s1').style.display = "none";
        document.getElementById('s2').style.display = "none";
        document.getElementById('syoukyo').style.display = "none";
      }
      function hyoujifunk(){
        document.getElementById('up').style.display = "block";
        document.getElementById('down').style.display = "block";
        document.getElementById('left').style.display = "block";
        document.getElementById('right').style.display = "block";
        document.getElementById('s1').style.display = "block";
        document.getElementById('s2').style.display = "block";
        document.getElementById('syoukyo').style.display = "block";
        document.getElementById('menu').style.display = "block";
      }
      function savefunk(){
        if(KAI==1){
          loadfunk();
        }else if(HP<=0){
          location.href = 'gameover.html';
        }else{
          localStorage.setItem('a',now);
          localStorage.setItem('b',nowpng);
          localStorage.setItem('c',i);
          localStorage.setItem('d',Start);
          localStorage.setItem('e',free);
          localStorage.setItem('f',slimeHP[0]);
          localStorage.setItem('g',slimeHP[1]);
          localStorage.setItem('h',slimeHP[2]);
          localStorage.setItem('i',demonHP[0]);
          localStorage.setItem('j',dragon);
          localStorage.setItem('k',box);
          localStorage.setItem('l',next);
          localStorage.setItem('n',KAI);
          localStorage.setItem('m',takara);
          localStorage.setItem('o',slime[0]);
          localStorage.setItem('p',slime[1]);
          localStorage.setItem('q',slime[2]);
          localStorage.setItem('r',demon[0]);
          localStorage.setItem('s',demon[1]);
          localStorage.setItem('t',demon[2]);
          localStorage.setItem('u',dragon);
          localStorage.setItem('v',Cslime[0]);
          localStorage.setItem('w',Cslime[1]);
          localStorage.setItem('x',Cslime[2]);
          localStorage.setItem('y',Cdemon[0]);
          localStorage.setItem('z',Cdemon[1]);
          localStorage.setItem('aa',Cdemon[2]);
          localStorage.setItem('ab',Cdragon);
          localStorage.setItem('ac',HP);
          localStorage.setItem('ad',ATK);
          localStorage.setItem('ae',MP);
          localStorage.setItem('af',count);
          localStorage.setItem('ag',hozon);
          localStorage.setItem('ah',tarn);
          localStorage.setItem('ai',Mwaza);
          localStorage.setItem('aj',clone);
          localStorage.setItem('ak',clone2);
          localStorage.setItem('al',move);
          localStorage.setItem('an',skil1);
          localStorage.setItem('am',skil2);
          localStorage.setItem('ao',demonHP[1]);
          localStorage.setItem('ap',demonHP[2]);
          localStorage.setItem('aq',dragonHP);
          localStorage.setItem('ar',LV);
          localStorage.setItem('as',nextLV);
          localStorage.setItem('at',keikenti);
          localStorage.setItem('au',MAXHP);
          localStorage.setItem('av',MAXMP);
          localStorage.setItem('aw',Rtakara);
          localStorage.setItem('ax',Thyouji);
          localStorage.setItem('ay',Bhyouji);
          localStorage.setItem('az',Bhantei);
          localStorage.setItem('aaa',kuuhaku);
          localStorage.setItem('aab',Dratakara);
          localStorage.setItem('aac',GETskil);
          localStorage.setItem('aad',GETmode);
          localStorage.setItem('aae',tokusyu);
          localStorage.setItem('aaf',comeKAI);
          localStorage.setItem('coin',coin);
          Start = document.getElementById("log");
          Start.innerHTML="セーブしました";
        }
      }
      function menufunk(){
        hihyoujifunk();
        document.getElementById('ura').style.display = "block";
        document.getElementById('shop').style.display = "block";
        document.getElementById('node').style.display = "block";
        document.getElementById('ritaia').style.display = "block";
        document.getElementById('tojiru').style.display = "block";
      }
      function tojirufunk(){
        document.getElementById('ura').style.display = "none";
        document.getElementById('shop').style.display = "none";
        document.getElementById('node').style.display = "none";
        document.getElementById('ritaia').style.display = "none";
        document.getElementById('tojiru').style.display = "none";
        if(HP<=0){
        }else{
          hyoujifunk();
        }
      }
      function ritaiafunk(){
        HP=0;
        gameoverfunk();
      }
